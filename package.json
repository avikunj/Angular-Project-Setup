{
  "name": "angular-setup",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "config": "ts-node ./scripts/setenv.js",
    "start:local": "env-cmd -f ./.env.local npm run config -- --environment=local  && ng serve",
    "start:dev": "env-cmd -f ./.env.development npm run config -- --environment=dev && ng serve",
    "start:prod": "env-cmd -f ./.env.production npm run config -- --environment=prod && ng serve",
    "build:local": "env-cmd -f ./.env.local npm run config -- --environment=local  && ng build",
    "build:dev": "env-cmd -f ./.env.development npm run config -- --environment=dev && ng build",
    "build:prod": "env-cmd -f ./.env.production npm run config -- --environment=prod && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:watch": "nodemon --exec \"npm run lint || exit 1\"",
    "format": "npx prettier src --write",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@ngrx/store": "^15.1.0",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "prettier-eslint": "^15.0.1",
    "rxjs": "~7.8.0",
    "save-dev": "^0.0.1-security",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "lint-staged": {
    "*.js|*.jsx|*.tsx|*.ts|*.html|*.css|*.scss": [
      "prettier --write", 
      "eslint --fix"
    ] 
   },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.1",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "~15.1.1",
    "@angular/compiler-cli": "^15.1.0",
    "@ngrx/eslint-plugin": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.1.0",
    "typescript": "~4.9.4",
    "yargs": "^17.6.2"
  }
}
